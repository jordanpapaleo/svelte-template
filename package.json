{
  "name": "jp-svelte-app",
  "description": "SvelteJS template with prettier and standard js",
  "version": "1.0.0",
  "scripts": {
    "build": "$(npm bin)/rollup -c",
    "dev": "$(npm bin)/rollup -c -w",
    "start": "$(npm bin)/sirv public -s",
    "test": "$(npm bin)/eslint '**/*.js' --quiet",
    "fix": "$(npm bin)/eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "format": "$(npm bin)/prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "babel-eslint": "10.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "eslint": "^7.18.0",
    "prettier-plugin-svelte": "^2.1.1",
    "prettier": "^2.0.5",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup": "^2.38.2",
    "svelte-router-spa": "^5.8.4",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "typescript": "^3.9.3",
    "tslib": "^2.0.0",
    "@tsconfig/svelte": "^1.0.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "1.12.0",
    "date-fns": "^2.17.0",
    "sirv-cli": "^1.0.0"
  }
}