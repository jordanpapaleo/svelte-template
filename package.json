{
  "name": "jp-svelte-app",
  "description": "SvelteJS template with prettier and standard js",
  "version": "1.0.0",
  "scripts": {
    "build": "$(npm bin)/rollup -c",
    "dev": "$(npm bin)/rollup -c -w",
    "start": "$(npm bin)/sirv public",
    "test": "$(npm bin)/eslint '**/*.js' --quiet",
    "fix": "$(npm bin)/eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "format": "$(npm bin)/prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "babel-eslint": "10.0.0",
    "eslint": "7.5.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.4.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "-": "0.0.1",
    "sirv-cli": "^1.0.0"
  }
}
